{
  "name": "Cordial",
  "author": {
    "name": "BeeBots",
    "email": "friendscottn@gmail.com",
    "url": "https://www.zendesk.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "domainWhitelist": ["api.usw2.cordial.io", "api.cordial.io"],
  "parameters": [
    {
      "name": "api_url",
      "type": "text",
      "secure": false,
      "required": true,
      "default": "https://api.usw2.cordial.io"
    },
    {
      "name": "api_token",
      "type": "text",
      "secure": true,
      "required": true
    },
    {
      "name": "allowed_contact_attributes",
      "type": "multiline",
      "secure": false,
      "required": false
    },
    {
      "name": "enable_events",
      "type": "checkbox",
      "secure": false,
      "required": false,
      "default": "false"
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}